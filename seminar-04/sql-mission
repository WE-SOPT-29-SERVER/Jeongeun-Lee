INSERT INTO post (user_id, title, content) VALUES(3, 'title', 'something')
RETURNING *;

UPDATE post
SET title = 'new title',
    updated_at = now()
WHERE post.id = 1
RETURNING *;

UPDATE post
SET is_deleted = true,
    updated_at = now()
WHERE post.id = 1
RETURNING *;

